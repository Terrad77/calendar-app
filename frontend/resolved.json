{
    "compilerOptions": {
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
        "target": "es2020",
        "useDefineForClassFields": true,
        "lib": [
            "es2020",
            "dom",
            "dom.iterable"
        ],
        "module": "esnext",
        "skipLibCheck": true,
        "moduleResolution": "node10",
        "allowImportingTsExtensions": true,
        "verbatimModuleSyntax": true,
        "moduleDetection": "force",
        "noEmit": true,
        "jsx": "react-jsx",
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "erasableSyntaxOnly": true,
        "noFallthroughCasesInSwitch": true,
        "noUncheckedSideEffectImports": true,
        "isolatedModules": true,
        "preserveConstEnums": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "strictBindCallApply": true,
        "strictPropertyInitialization": true,
        "strictBuiltinIteratorReturn": true,
        "alwaysStrict": true,
        "useUnknownInCatchVariables": true
    },
    "files": [
        "./src/App.tsx",
        "./src/global.d.ts",
        "./src/main.tsx",
        "./src/vite-env.d.ts",
        "./src/API/apiOperations.ts",
        "./src/API/axiosInstance.ts",
        "./src/components/AIAssistant/AIAssistant.styles.ts",
        "./src/components/AIAssistant/AIAssistant.tsx",
        "./src/components/Calendar/Calendar.tsx",
        "./src/components/Calendar/CalendarDayCell.tsx",
        "./src/components/Calendar/CalendarGridHeader.tsx",
        "./src/components/Calendar/CalendarHeader.tsx",
        "./src/components/Calendar/SearchInput.tsx",
        "./src/components/Calendar/TaskCardDraggable.tsx",
        "./src/components/Calendar/TaskInputForm.tsx",
        "./src/components/Icon/index.tsx",
        "./src/components/UserProfile/UserProfileHeader.tsx",
        "./src/redux/store.ts",
        "./src/redux/task/operations.ts",
        "./src/redux/task/selectors.ts",
        "./src/redux/task/slice.ts",
        "./src/redux/task/types.ts",
        "./src/redux/user/operations.ts",
        "./src/redux/user/selectors.ts",
        "./src/redux/user/slice.ts",
        "./src/redux/user/types.ts",
        "./src/services/aiService.ts",
        "./src/services/authService.ts",
        "./src/types/react-hot-toast.d.ts",
        "./src/types/types.ts",
        "./src/utils/date.ts",
        "./src/utils/toastMaker/toastMaker.tsx"
    ],
    "include": [
        "src",
        "src/types",
        "src/global.d.ts"
    ]
}
